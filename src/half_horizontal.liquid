<div class="layout">
    {% if trmnl.plugin_settings.custom_fields_values.source == 'cbc' %}
        {% assign feedTitle = IDX_0.feed.title %}
        {% assign feedItems = IDX_0.items %}

        {% if trmnl.plugin_settings.custom_fields_values.time_format == '24h' %}
            {% assign pubDateFmt = '%B %d %Y %H:%M' %}
        {% else  %}
            {% assign pubDateFmt = '%B %d %Y %l:%M %P' %}
        {% endif %}
    {% else %}
        {% assign feedTitle = IDX_1.feed.title %}
        {% assign feedItems = IDX_1.items %}

        {% assign pubDateFmt = '%d %B %Y %H h %M' %}
    {% endif %}

    <div class="columns">
        <div class="column"
            data-list-limit="true"
            data-list-hidden-count="true"
            data-list-max-columns="1"
            >
            {% for story in feedItems %}
                {% comment %} Skip the image that appears at the start of the content in order to get the text {% endcomment %}
                {% assign articleParts = story.content | split: '<p>' %}
                {% assign articleText = articleParts[1] %}

                <div class="item">
                    <div class="meta"></div>
                    <div class="content">
                        <span class="title title--small clamp--2">{{ story.title }}</span>
                        <div class="flex gap--small">
                            <span class="label label--small label--underline">{{ story.pubDate | date: '%s' | plus: trmnl.user.utc_offset | date: pubDateFmt }}</span>
                        </div>
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>
</div>

<div class="title_bar">
  <img class="image" src="data:image/png;base64," />
    <span class="title">{% if trmnl.plugin_settings.custom_fields_values.source == 'cbc' %}CBC{% else %}Radio-Canada{% endif %}</span>
    <span class="instance">{{ feedTitle }}</span>
</div>